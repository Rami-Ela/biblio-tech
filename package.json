{
  "name": "biblio-tech",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "yarn prettier --plugin=prettier-plugin-sql --check src",
    "format:fix": "yarn prettier --plugin=prettier-plugin-sql --write src",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest --passWithNoTests",
